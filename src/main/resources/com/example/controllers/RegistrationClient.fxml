<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllers.RegistrationSceneController">
   <children>
      <Button layoutX="263.0" layoutY="361.0" mnemonicParsing="false" onAction="#clickOnSignUp" text="SignUp">
         <font>
            <Font name="Palatino Linotype" size="12.0" />
         </font></Button>
      <TextField fx:id="FirstNameRegistrationField" layoutX="226.0" layoutY="67.0" promptText="FirstName*" />
      <TextField fx:id="SurnameRegistrationField" layoutX="226.0" layoutY="115.0" promptText="Surname*" />
      <TextField fx:id="EMailRegistrationField" layoutX="226.0" layoutY="167.0" promptText="Email*" />
      <TextField fx:id="LoginRegistrationField" layoutX="226.0" layoutY="222.0" promptText="Login*" />
      <PasswordField fx:id="PasswordRegistrationField" layoutX="226.0" layoutY="277.0" promptText="Password*" />
      <Label layoutX="213.0" layoutY="14.0" text="REGISTRATION">
         <font>
            <Font name="Palatino Linotype" size="23.0" />
         </font>
      </Label>
      <Label layoutX="226.0" layoutY="317.0" text="* - required fields" />
      <Label fx:id="WarningLabel" layoutX="226.0" layoutY="344.0" textFill="RED" wrapText="true" />
      <Button fx:id="BackButton" layoutX="23.0" layoutY="17.0" mnemonicParsing="false" onAction="#clickedOnBack" text="Back" />
   </children>
</AnchorPane>
